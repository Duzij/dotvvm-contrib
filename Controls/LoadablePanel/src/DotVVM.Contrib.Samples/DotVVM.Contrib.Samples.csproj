<Project ToolsVersion="15.0" Sdk="Microsoft.NET.Sdk.Web">
	<PropertyGroup>
		<TargetFramework>netcoreapp2.0</TargetFramework>
	</PropertyGroup>
	<ItemGroup>
		<Content Include="**\*.dothtml" />
	</ItemGroup>
	<ItemGroup>
	  <Compile Remove="wwwroot\NewFolder\**" />
	  <Content Remove="wwwroot\NewFolder\**" />
	  <EmbeddedResource Remove="wwwroot\NewFolder\**" />
	  <None Remove="wwwroot\NewFolder\**" />
	</ItemGroup>
	<ItemGroup>
		<None Remove="Controls\Pager\Pager.js" />
		<None Remove="dotvvm_serialized_config.json.tmp" />
		<None Remove="Views\PagingRepeater.dothtml" />
		<None Remove="Views\Sample2.dothtml" />
		<None Remove="Views\Sample3.dothtml" />
	</ItemGroup>
	<ItemGroup>
	  <Content Include="Controls\Pager\Pager.js">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Content>
	  <Content Include="Views\_master.dotmaster" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore" Version="2.0.2" />
		<PackageReference Include="Microsoft.AspNetCore.StaticFiles" Version="2.0.2" />
		<PackageReference Include="DotVVM.AspNetCore" Version="$(DotvvmVersion)" />
	</ItemGroup>
  <ItemGroup>
    <DotNetCliToolReference Include="DotVVM.Compiler.Light" Version="1.1.6.3" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\DotVVM.Contrib\DotVVM.Contrib.csproj" />
  </ItemGroup>
</Project>