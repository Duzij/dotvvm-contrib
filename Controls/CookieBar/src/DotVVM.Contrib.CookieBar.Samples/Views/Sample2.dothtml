@viewModel DotVVM.Contrib.CookieBar.Samples.ViewModels.Sample2ViewModel, DotVVM.Contrib.CookieBar.Samples
@masterPage Views/_master.dotmaster

<dot:Content ContentPlaceHolderID="MainContent">

    <input type="button" onclick="DotVVM.Contrib.CookieBar.resetConsent(true);" value="Reset consent" ID="reset"/>

    <dc:CookieBar>
        <dc:GoogleAnalyticsRule />
        <dc:GoogleAdsRule />
        <dc:FacebookPixelRule />
        <dc:SmartlookRule />
    </dc:CookieBar>

</dot:Content>

